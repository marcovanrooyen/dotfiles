{
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "[snippets]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.commandCenter.enabled": false,
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    "**/dist/**"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.readonlyInclude": {
    "**/.cargo/git/checkouts/**/*.rs": true,
    "**/.cargo/registry/src/**/*.rs": true,
    "**/lib/rustlib/src/rust/library/**/*.rs": true
  },
  "git.blame.editorDecoration.enabled": true,
  "gitlens.views.scm.grouped.views": {
    "branches": false,
    "commits": false,
    "contributors": false,
    "launchpad": false,
    "remotes": false,
    "repositories": false,
    "searchAndCompare": false,
    "stashes": false,
    "tags": false,
    "worktrees": false
  },
  "javascript.suggest.jsdoc.generateReturns": false,
  "jsdoc-generator.descriptionPlaceholder": "",
  "jsdoc-generator.emptyLineAfterHeader": false,
  "jsdoc-generator.generativeModel": "gpt-4o",
  "jsdoc-generator.includeAsync": false,
  "jsdoc-generator.includeExport": false,
  "jsdoc-generator.includeReturn": false,
  "terminal.integrated.fontLigatures.enabled": true,
  "typescript.suggest.completeJSDocs": false,
  "typescript.suggest.jsdoc.generateReturns": false,
  "update.showReleaseNotes": false,
  "vim.highlightedyank.color": "#CBA6F7",
  "vim.highlightedyank.duration": 200,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // NICE TO HAVE
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.saveFiles"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    }
  ],
  "vim.overrideCopy": true,
  "vim.scroll": 5,
  "vim.timeout": 5000,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.codeAction"]
    },
    // Stay in visual mode while indenting
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    // Paste without updating register
    {
      "after": ["p", "g", "v", "y"],
      "before": ["p"]
    }
  ],
  "window.commandCenter": false,
  "window.nativeTabs": true,
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.pullRequests.enabled": false,
  "gitlens.graph.scrollMarkers.enabled": false,
  "gitlens.graph.showDetailsView": false,
  "gitlens.graph.showGhostRefsOnRowHover": false,
  "gitlens.graph.highlightRowsOnRefHover": false,
  "gitlens.graph.showUpstreamStatus": false,
  "gitlens.graph.issues.enabled": false,
  "gitlens.graph.pullRequests.enabled": false,
  "gitlens.graph.avatars": false,
  "gitlens.graph.dateStyle": "absolute",
  "gitlens.views.commits.pullRequests.showForBranches": false,
  "abracadabra.extractInterface.showInQuickFix": false,
  "abracadabra.extractGenericType.showInQuickFix": false,
  "abracadabra.createFactoryForConstructor.showInQuickFix": false,
  "abracadabra.convertToTemplateLiteral.showInQuickFix": false,
  "sort-json.settings.listSortType": "Value",
  "csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
  "javascript.suggest.completeFunctionCalls": true,
  "typescript.suggest.completeFunctionCalls": true,
  "editor.snippetSuggestions": "bottom",
  "editor.quickSuggestions": {
    "strings": "inline"
  },
  "editor.inlineSuggest.edits.renderSideBySide": "never",
  "editor.inlineSuggest.edits.showCollapsed": true,
  "editor.suggest.insertMode": "replace",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "javascript.updateImportsOnFileMove.enabled": "always"
}
